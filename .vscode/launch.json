  "version": "0.2.0",
  "configurations": [
    {
      "name": "run core",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build core",
      "program": "${workspaceFolder}/Antiriad.Core/bin/Debug/net7.0/Antiriad.Core",
      "args": [],
      "cwd": "${workspaceFolder}/Antiriad.Core",
      "console": "externalTerminal",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}